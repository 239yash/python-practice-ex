def spy_game(arr):
    c = 0
    k = 0
    for num in arr:
        if c <= 2:
            if num==0:
                c = c + 1
                continue
        if k <= 1:
            if num == 7:
                k = k + 1
                break
    if c ==2 and k == 1:
        print('true')

    else:
        print('false')        





spy_game([1,0,0,4,5,6,7,8])
